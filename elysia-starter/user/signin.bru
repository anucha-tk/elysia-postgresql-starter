meta {
  name: signin
  type: http
  seq: 3
}

post {
  url: {{url}}/user/sign-in
  body: json
  auth: inherit
}

params:query {
  ~asda: asd
}

body:json {
  {
    "email": "admin@gmail.com",
    "password": "12345678"
  }
}

script:post-response {
  let responseBody = res.getBody();
  let token = responseBody.data.token
  bru.setEnvVar("authToken", token);
}

settings {
  encodeUrl: true
  timeout: 0
}
